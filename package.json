{
  "name": "react-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React SSR Boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --config webpack.prod.js",
    "dev": "npm-run-all --parallel server client:dev",
    "prod": "npm-run-all --parallel server client:prod",
    "server": "nodemon index.js",
    "client:dev": "webpack --watch --config webpack.dev.js",
    "client:prod": "webpack --watch --config webpack.prod.js",
    "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json"
  },
  "author": "Krzysztof Perzanowski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-transform-modules-commonjs": "7.7.5",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.7",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "clean-webpack-plugin": "3.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-react": "7.17.0",
    "jest": "24.9.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "7.7.7",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>__tests__/setup/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
