{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/display-name": [0, { "ignoreTranspilerName": true }],
    "no-console": 2
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier/react"
  ],
  "settings": {
    "react": {
      "version": "16.13.1"
    }
  }
}